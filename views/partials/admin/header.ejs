  }

  .screen-overlay {
    display: none;
    position: fixed;
    top: 0;
    left: 0;
    width: 100vw;
    height: 100vh;
    background-color: rgba(0, 0, 0, 0.5);
    z-index: 999;
  }

  .screen-overlay.active {
    display: block;
  }

  .main-wrap {
    margin-left: 0 !important;
  }




  .main-header .col-nav {
    display: flex;
    align-items: center;
    justify-content: space-between;
  }



}

